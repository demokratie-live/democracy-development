{
  "name": "queue-pushs-vote-top-100",
  "version": "0.1.15",
  "main": "build/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess 'node build/index.js'",
    "build": "tsup-node",
    "lint": "pnpm lint:ts && pnpm lint:exports",
