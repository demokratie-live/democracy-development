  "dependencies": {
    "@democracy-deutschland/democracy-common": "workspace:*",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.13",
    "ts-unused-exports": "^6.3.0",
    "tsconfig": "workspace:*",
    "tsup": "catalog:",
    "tsup-config": "workspace:*",
    "typescript": "^5.5.4"
  }
}
