    "scrape-it": "^6.1.4"
  },
  "devDependencies": {
    "dotenv": "^16.4.7",
    "ts-node-dev": "^2.0.0",
    "ts-unused-exports": "^11.0.1",
    "tsx": "^4.11.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  }
