    "p-iteration": "^1.1.8"
  },
  "devDependencies": {
    "@types/node-gcm": "^1.0.0",
    "apollo": "^2.28.3",
    "ts-node-dev": "^1.0.0-pre.49",
    "ts-unused-exports": "^6.2.1",
    "typescript": "^3.9.5"
  }
}
