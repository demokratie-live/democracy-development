{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "project": ["src/**/*.ts"],
  "ignore": ["src/generated/**", "src/types/*.d.ts"],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "ignoreDependencies": ["@types/mongoose", "babel-jest", "dotenv-cli", "ts-jest"],
  "jest": {
    "config": ["jest.unit.config.js", "jest.integration.config.js"],
    "entry": ["jest.unit.setup.js", "jest.integration.setup.js"]
  },
  "eslint": true,
  "babel": true,
  "prettier": true,
  "typescript": true
}
