{
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/ios": true,
    "**/android": true,
    "**/.next": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true
}
