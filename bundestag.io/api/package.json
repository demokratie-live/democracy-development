    "axios": "^0.19.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "eslint": "^7.4.0",
    "express": "^4.16.2",
    "express-status-monitor": "^1.2.3",
    "graphql": "^15.2.0",
    "graphql-date": "^1.0.3",
    "lodash": "^4.17.10",
    "merge-graphql-schemas": "^1.5.0",
    "mongoose": "^5.9.24",
    "mongoose-diff-history": "^2.1.0",
    "winston": "^3.0.0",
    "winston-mongodb": "^5.0.1",
    "xml-js": "^1.6.11"
